<%= form_with(url: path,scope: :session, local:true) do |f| %>
    <p>
        <%= f.label :email, "E-mail:" %>
        <%= f.text_field :email %>
    </p>
    <p>
        <%= f.label :password, "Password:" %>
        <%= f.password_field :password %>
    </p>

    <%= f.submit button_message%>
<% end %>

